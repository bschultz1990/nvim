{"confirm (binary user choice)":{"body":["$confirmed = $false","while ($confirmed -eq $false)","{","  Write-Host \"${1:#do something}\"","  $confirm = Read-Host \"${2:#choice} ($3/$4)\"","  if ($confirm.ToLower() -eq '$3')","  {","    ${5:#do-something}","  } elseif ($confirm.ToLower() -eq '$4')","  {","    ${0:#do-something-else}","    $confirmed = $true","  }","","}"],"prefix":"confirm (binary user choice)"},"requirements":{"prefix":"requirements","body":"if ((requirements('${1:app}')) -eq 1) { return }"}}