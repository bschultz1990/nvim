set winheight=1
set winminwidth=0
set winwidth=1
exe 'vert 1resize ' . ((&columns * 128 + 128) / 256)
exe 'vert 2resize ' . ((&columns * 127 + 128) / 256)
argglobal
enew | setl bt=help
help :split@en
balt Projects/dieRoller3/styles.css
setlocal fdm=manual
